@model IEnumerable<Position>

@foreach (var item in Model)
{
    if (item.Id == ViewBag.PositionId)
    {
        <input type="radio" name="PositionId" value="@item.Id" checked /> @item.Value<br />
    }
    else
    {
        <input type="radio" name="PositionId" value="@item.Id" /> @item.Value<br />
    }
}